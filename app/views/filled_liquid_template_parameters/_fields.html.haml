.nested-fields
	= f.input :name, label: 'Parameter Name', as: :string, wrapper_html: { class: 'filled-liquid-template-parameter-name' }
	= f.input :value, label: 'Parameter Value', as: :string, wrapper_html: { class: 'filled-liquid-template-parameter-value' }
	= link_to_remove_association "Remove Parameter", f, class: 'btn btn-default'